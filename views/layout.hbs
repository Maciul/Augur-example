<!DOCTYPE html>
<html>
  <head>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
  <script src="/javascripts/fingerprint.js"></script>
  <script src="/javascripts/fingerprint2.js"></script>
  <link href="https://fonts.googleapis.com/css?family=Lato" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Indie+Flower" rel="stylesheet">

    <script>
        function callbackName( json ) {  
          console.log(json);
            $(function() {
              $('#augurIDs').append( '<p>Device ID:</p> ' + '<p>' + augur.device.ID + '<p>' + '<p>Customer ID:</p> ' + '<p>' + augur.consumer.UID + '<p>'  );
            }); 
        console.log( augur.device.ID ); 
        }   
    </script>
    <script
        async
        id="augur.js"
        data-callback="callbackName" 
        src="//cdn.augur.io/augur.min.js"
        data-warpspeed="{{warp}}"
        data-sendto="POST::http://testing123.augur.io/">
    </script>
    <script> 
        var fingerprint = new Fingerprint().get();
        var fingerprint2 = new Fingerprint({canvas: true}).get();
        var fingerprint3 = new Fingerprint({screen_resolution: true}).get();
      $(function() {
        $('#fingerprintjs').append('<p>Device ID:</p> ' + '<p>' + fingerprint + '<p>' );
      });
    </script>
    <script> 
        var fingerprintjs2 = new Fingerprint2();

      fingerprintjs2.get(function(result) {
        if (typeof window.console !== "undefined") {

        $(function() {
         $('#fingerprintjs2').append('<p>Device ID:</p>' + '<p>' + result + '<p>' );
        });
        }
      });
    </script>

    
    <title>"Augur Example"</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    {{{body}}}
  </body>
</html>

